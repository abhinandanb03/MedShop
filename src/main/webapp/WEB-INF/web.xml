<?xml version="1.0" encoding="UTF-8"?>
<web-app>
	<display-name>MedShop</display-name>
	<servlet>
		<servlet-name>login</servlet-name>
        <servlet-class>com.abhi.loginform</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>validate</servlet-name>
        <servlet-class>com.abhi.Validate</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>signup</servlet-name>
        <servlet-class>com.abhi.signup</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>login</servlet-name>
        <url-pattern>/loginform</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>validate</servlet-name>
        <url-pattern>/Validate</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>signup</servlet-name>
        <url-pattern>/signup</url-pattern>
	</servlet-mapping>
</web-app>
